<p id="notice"><%= notice %></p>

<h1>Listing Program Chefs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @program_chefs.each do |program_chef| %>
      <tr>
        <td><%= link_to 'Show', program_chef %></td>
        <td><%= link_to 'Edit', edit_program_chef_path(program_chef) %></td>
        <td><%= link_to 'Destroy', program_chef, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

